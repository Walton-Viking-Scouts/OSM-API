  /events/event/sharing/:
    post:
      summary: POST sharing
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  has_myscout:
                    type: boolean
                  approval_status:
                    type: object
                    nullable: true
                  limit:
                    type: number
                  leaders:
                    type: boolean
                  shareDownloads:
                    type: boolean
                  shareConfDateLockdown:
                    type: boolean
                  shareDetailsLockdown:
                    type: boolean
                  fields:
                    type: array
                    items:
                      type: object
                      properties:
                        id:
                          type: string
                        name:
                          type: string
                        pL:
                          type: string
                        pR:
                          type: string
                  eventJson:
                    type: object
                    properties:
                      eventid:
                        type: string
                      allowchanges:
                        type: number
                      allowbooking:
                        type: number
                      disablereminders:
                        type: number
                      confdate:
