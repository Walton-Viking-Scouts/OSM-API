  /events/summary/:
    get:
      summary: GET summary
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  identifier:
                    type: string
                  items:
                    type: array
                    items:
                      type: object
                      properties:
                        eventid:
                          type: string
                        name:
                          type: string
                        date:
                          type: string
                        startdate_g:
                          type: string
                        startdate:
                          type: string
                        enddate:
                          type: string
                        starttime:
                          type: string
                        endtime:
                          type: string
                        cost:
                          type: string
                        location:
                          type: string
                        approval_status:
                          type: object
                          nullable: true
                        rota_offered:
                          type: number
                        rota_accepted:
                          type: number
                        rota_required:
                          type: object
                          nullable: true
                        yes:
                          type: number
                        yes_members:
                          type: number
                        yes_yls:
                          type: number
                        yes_leaders:
                          type: number
                        reserved:
                          type: number
                        no:
                          type: number
                        invited:
                          type: number
                        shown:
                          type: number
                        x:
                          type: number
