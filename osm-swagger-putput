  /badges/stock/:
    get:
      summary: GET stock
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  identifier:
                    type: string
                  items:
                    type: array
                    items:
                      type: object
                      properties:
                        _filterString:
                          type: string
                        shortname:
                          type: string
                        badge_id_level:
                          type: string
                        name:
                          type: string
                        picture:
                          type: string
                        stock:
                          type: number
                        desired:
                          type: number
                        badge_id:
                          type: string
                        level:
                          type: number
                        buy:
                          type: number
                        due:
                          type: number
                        shared_by:
                          type: string
                        shared:
                          type: string
              example:
                identifier: badge_id_level
                items:
                - _filterString: chief scouts bronze
                  shortname: '1529_0_1'
