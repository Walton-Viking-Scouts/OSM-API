  /members/flexirecords/:
    get:
      summary: GET flexirecords
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  identifier:
                    type: string
                  label:
                    type: string
                  items:
                    type: array
                    items:
                      type: object
                      properties:
                        extraid:
                          type: string
                        name:
                          type: string
              example:
                identifier: extraid
                label: name
                items:
                - extraid: '59145'
                  name: Lodge
                - extraid: '54696'
                  name: qdfasdas
                - extraid: '54487'
                  name: Scarves
                - extraid: '72758'
                  name: Viking Event Mgmt
      parameters:
      - name: action
        in: query
        required: false
        schema:
          type: string
      - name: sectionid
        in: query
        required: false
        schema:
          type: number
      - name: archived
        in: query
        required: false
        schema:
          type: string
    post:
      summary: POST flexirecords
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  items:
                    type: array
                    items:
                      type: object
                      properties:
                        f_1:
                          type: number
                        scoutid:
                          type: number
              example:
                items:
                - f_1: 2
                  scoutid: 1809627
      parameters:
      - name: action
        in: query
        required: false
        schema:
          type: string
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              type: object
              properties:
                termid:
                  type: string
                section:
                  type: string
                sectionid:
                  type: string
                extraid:
                  type: string
                scoutid:
                  type: string
                column:
                  type: string
                value:
                  type: string
            example:
              termid: '813460'
              section: beavers
              sectionid: '49097'
              extraid: '72758'
              scoutid: '1809627'
              column: f_1
              value: '2'
