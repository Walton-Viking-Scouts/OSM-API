  /users/auth/:
    post:
      summary: POST auth
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  now_ts:
                    type: number
                  security_question:
                    type: string
                  security_letters:
                    type: array
                    items:
                      type: number
                  security_recoverable:
                    type: boolean
                  security_number:
                    type: string
              example:
                now_ts: 1752695675
                security_question: What was the model of your first car?
                security_letters:
                - 0
                - 1
                - 2
                security_recoverable: true
                security_number: +4477******56
      parameters:
      - name: action
        in: query
        required: false
        schema:
          type: string
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              type: object
              properties:
                email:
                  type: string
                password:
                  type: string
                device_id:
                  type: string
                seen:
                  type: string
            example:
              email: simon@waltonviking.uk
              password: nerta8-gyZxux-mepnuj
              device_id: d71a33b6-3e64-4139-94bd-fe6db26be18b
              seen: internal 2
