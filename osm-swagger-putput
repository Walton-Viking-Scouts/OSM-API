    post:
      summary: POST stock
      responses:
        '200':
          description: ''
          content:
            application/json:
              schema:
                type: object
                properties:
                  ok:
                    type: boolean
              example:
                ok: true
      parameters:
      - name: action
        in: query
        required: false
        schema:
          type: string
      requestBody:
        content:
          application/x-www-form-urlencoded:
            schema:
              type: object
              properties:
                stock:
                  type: string
                badge_id:
                  type: string
                level:
                  type: string
                section_id:
                  type: string
                section:
                  type: string
                type:
                  type: string
            example:
              stock: '5'
              badge_id: '1529'
              level: '1'
              section_id: '49097'
              section: beavers
              type: desired
